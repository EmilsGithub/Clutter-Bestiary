{
	"schemaVersion": 1,
	"id": "clutterbestiary",
	"version": "${version}",
	"name": "Clutter: Bestiary",
	"description": "The entities from Clutter, plus more, updated, improved and split into a separate mod.",
	"authors": [
		"EmilSG"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/clutterbestiary/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.emilsg.clutterbestiary.ClutterBestiary"
		],
		"client": [
			"net.emilsg.clutterbestiary.ClutterBestiaryClient"
		],
		"fabric-datagen": [
			"net.emilsg.clutterbestiary.ClutterBestiaryDataGenerator"
		]
	},
	"mixins": [
		"clutterbestiary.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.17.0",
		"minecraft": "~1.21.1",
		"java": ">=21",
